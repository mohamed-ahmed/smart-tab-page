{
	"name": "Smart New Tab Page",
	"description": "Creats app icons for your most visited sites",

	"permissions": ["topSites", "<all_urls>"],
	"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["javascript/jquery-2.1.0.min.js", "javascript/popup.js"]
    }],


	"version": "0.1",
	"incognito": "split",
	"chrome_url_overrides": {
		"newtab": "home.html"
		},
		"manifest_version": 2
	}
